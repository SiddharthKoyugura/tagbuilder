<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.assetsense.tagbuilder.c2.domain.Model" table="MODEL">
		<id name="id" column="id" type="long">
			<generator class="native" />
		</id>

		<property name="name" column="name" not-null="true" type="string"></property>

		<many-to-one name="assetType" column="asset_type"
			class="com.assetsense.tagbuilder.c2.domain.Lookup" not-null="false"
			fetch="select" lazy="false" />
			
		<many-to-one name="supplierName" column="supplier_name"
			class="com.assetsense.tagbuilder.c2.domain.Lookup" not-null="false"
			fetch="select" lazy="false" />
			
		<many-to-one name="assetCategory" column="asset_category"
			class="com.assetsense.tagbuilder.c2.domain.Lookup" not-null="false"
			fetch="select" lazy="false" />
	</class>
</hibernate-mapping>