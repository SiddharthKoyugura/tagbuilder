<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.assetsense.tagbuilder.pi.domain.Element" table="elements">
		<id name="id" column="id" type="long">
			<generator class="native"/>
		</id>

		<property name="name" column="name" not-null="false" type="string"></property>
		<!--<many-to-one name="childElement" column="childElements" not-null="false" />
		<many-to-one name="attribute" column="attribute" not-null="false"/>-->
	</class>
</hibernate-mapping>