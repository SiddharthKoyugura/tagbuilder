<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.assetsense.tagbuilder.c2.domain.Measurement" table="measurement">
		<id name="id" column="id" type="long">
			<generator class="native"/>
		</id>

		<property name="unitid" column="unitid" not-null="false" type="string"></property>
		<property name="name" column="name" not-null="false" type="string"></property>
	</class>
</hibernate-mapping>