<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.assetsense.tagbuilder.c2.domain.Observation" table="OBSERVATION">
		<id name="id" column="id" type="long">
			<generator class="native"/>
		</id>

		<property name="code" column="code" not-null="false" unique="true" type="string"></property>
		<!--<property name="inputType" column="inputType" not-null="false" type="string"></property>-->
		<many-to-one name="inputType" column="inputType" class="com.assetsense.tagbuilder.c2.domain.Lookup" not-null="false" />
		<property name="description" column="description" not-null="false" type="string"></property>
		<many-to-one name="category" column="category" class="com.assetsense.tagbuilder.c2.domain.Lookup" not-null="false" />
		<many-to-one name="measurement" column="measurement" class="com.assetsense.tagbuilder.c2.domain.Measurement" not-null="false" />
		<many-to-one name="unitid" column="unitid" class="com.assetsense.tagbuilder.c2.domain.Lookup" not-null="false" />
	</class>
	
</hibernate-mapping>